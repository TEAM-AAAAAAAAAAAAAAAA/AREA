{
  "name": "area-discord-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "npx tsc",
    "build:changelog": "npx @discordx/changelog --src src",
    "dev": "dotenv -e ./env/.env.dev -- npx nodemon -r dotenv/config --exec ts-node-esm src/main.ts",
    "test": "dotenv -e .env/.env.test -- node -r dotenv/config dist/main.js",
    "prod": "node dist/main.js"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/pagination": "^3.4.0",
    "@prisma/client": "4.9",
    "discord.js": "^14.7.1",
    "discordx": "^11.7.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "prisma": "4.9"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
